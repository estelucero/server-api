{
    "version": 2,
    "builds": [
      {
        "src": "server.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production"
    }
  }